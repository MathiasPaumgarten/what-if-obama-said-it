{
    "name": "what-if-obama-said-it",
    "private": true,
    "version": "0.0.1",
    "description": "Retweet headlines with Obama as the subject over Trump",
    "scripts": {
        "build": "tsc",
        "dev-client": "concurrently \"tsc -w\" \"watchify dist/client/app.js -o dist/client/bundle.js\"",
        "dev": "concurrently \"tsc -w\" \"watchify dist/client/app.js -o dist/client/bundle.js\" \"nodemon dist/server/index.js\"",
        "server": "npm run build && npm start",
        "start": "node dist/server/index.js",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.114",
        "@types/node": "^10.5.3",
        "@types/react": "^16.4.7",
        "@types/react-dom": "^16.0.6",
        "concurrently": "^3.6.1",
        "nodemon": "^1.18.3",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "tslint": "^5.11.0",
        "typescript": "^2.9.2",
        "watchify": "^3.11.0"
    },
    "dependencies": {
        "express": "^4.16.3",
        "pug": "^2.0.3",
        "rxjs": "^6.2.2",
        "twitter-js-client": "0.0.5"
    }
}
