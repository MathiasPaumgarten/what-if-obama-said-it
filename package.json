{
    "name": "what-if-obama-said-it",
    "private": true,
    "version": "0.0.1",
    "description": "Retweet headlines with Obama as the subject over Trump",
    "scripts": {
        "build": "tsc",
        "dev": "concurrently \"tsc -w\" \"watchify dist/client/app.js -o dist/client/bundle.js\" \"npm run less\" \"npm run server\" \"npm run assets\"",
        "server": "npm run build && node dist/server/index.js --fixtures",
        "start": "node dist/server/index.js",
        "less": "autoless styles/ dist/client/",
        "clean": "rm -rf dist",
        "assets": "cp -r assets dist/client/assets",
        "minify": "uglifyjs dist/client/bundle.js -o dist/client/bundle.js",
        "deploy": "npm run minify && gcloud app deploy --project=what-if-obama-said-it"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.6",
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.116",
        "@types/node": "^10.5.3",
        "@types/react": "^16.4.7",
        "@types/react-dom": "^16.0.6",
        "autoless": "^0.1.7",
        "classnames": "^2.2.6",
        "concurrently": "^3.6.1",
        "less": "^3.8.0",
        "lodash": "^4.17.10",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "tslint": "^5.11.0",
        "typescript": "^2.9.2",
        "uglifyjs": "^2.4.11",
        "watchify": "^3.11.0"
    },
    "dependencies": {
        "express": "^4.16.3",
        "pug": "^2.0.3",
        "rxjs": "^6.2.2",
        "twitter-js-client": "0.0.5"
    }
}
