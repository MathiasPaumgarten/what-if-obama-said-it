{
    "name": "what-if-obama-said-it",
    "private": true,
    "version": "0.0.1",
    "description": "Retweet headlines with Obama as the subject over Trump",
    "scripts": {
        "dev": "concurrently \"tsc -w\" \"watchify dist/client/app.js -o dist/client/bundle.js\" \"npm run less\" \"npm run server\" \"npm run assets\"",
        "server": "tsc && nodemon dist/server/index.js --fixtures",
        "start": "node dist/server/index.js",
        "less": "autoless styles/ dist/client/",
        "clean": "rm -rf dist",
        "assets": "cp -r assets dist/client/assets",
        "minify": "uglifyjs dist/client/bundle.js -o dist/client/bundle.js",
        "deploy": "npm run minify && gcloud app deploy --project=what-if-obama-said-it"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.6",
        "@types/express": "^4.16.0",
        "@types/express-session": "^1.15.11",
        "@types/google-cloud__datastore": "^1.3.3",
        "@types/lodash": "^4.14.116",
        "@types/node": "^10.10.1",
        "@types/passport": "^0.4.6",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.7",
        "autoless": "^0.1.7",
        "classnames": "^2.2.6",
        "concurrently": "^4.0.1",
        "less": "^3.8.1",
        "lodash": "^4.17.11",
        "nodemon": "^1.18.4",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3",
        "uglifyjs": "^2.4.11",
        "watchify": "^3.11.0"
    },
    "dependencies": {
        "@google-cloud/datastore": "^1.4.2",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0",
        "pug": "^2.0.3",
        "rxjs": "^6.3.2",
        "twitter-js-client": "0.0.5"
    }
}
